/* pages/goods/goodsList/goodsList.wxss */
.fl{
  float: left;
}
.fr{
  float: right;
}
.clearfix:after{
  content: ".";
  display: block;
  clear: both;
  visibility: hidden;
  font-size: 0;
}
page{
  background-color: #f4f5f7;
}

/* 主要内容 */
.container{
  margin-top: 92rpx;
}
/* 左侧内容  */
.leftScroll{
  width: 24%;
  height: 800rpx;
  background-color: #fff;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.leftScroll view{
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;  
  border-bottom: 1rpx solid #f1f1f1;
}
.leftScroll view text{
  font-size: 26rpx;
  color: #323232;
}
.leftScroll .topic{
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;  
  border-bottom: 1rpx solid transparent;
  background-color:#f4f5f7;
}
 .leftScroll .topic text{
  width: 100%;
  font-size: 26rpx;
  color: #f51322;
} 
/* 右侧内容 */
.container .right{
  width: 70%;
  margin-right: 3%;
}
.swiper {
  height: 176rpx;
  width: 100%;
  border: 1rpx solid #d9d9d9;
}
.swiper image {
  height: 100%;
  width: 100%;
}
.detail .topic{
  height: 92rpx;
  margin-bottom: 2rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: 0rpx 0rpx;
  background-size: 100% 92rpx;
  text-align: center;
  line-height: 100rpx;
}
.detail .topic text{
  margin-left: 44rpx;
  font-size: 32rpx;
  color: #f72543;
}
.rightScroll{
  height: 600rpx;
  background-color: #fff;  
}
.rightScroll .goods{
  width: 33%;
  height: 220rpx;
  float: left;
  border-left: 1rpx solid #f1f1f1;
  background-color: #fff;
}
.rightScroll .goods:nth-child(3n-2){
  border: none;
}
.goods .gImg{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 110rpx;
  padding: 20rpx 20rpx;
}
.gImg image{
  width: 100%;
  height: 100%;
}
.goods .gDespriction{
  width: 100%; 
  height: 74rpx;   
  text-align: center;
}
.gDespriction .gName{
  font-size: 24rpx;
  /* overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap; */
  color: #696969;
}